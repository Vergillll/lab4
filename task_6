print("input A1")
A1 = float(input())
print("input B1")
B1 = float(input())
print("input C1")
C1 = float(input())
print("input A2")
A2 = float(input())
print("input B2")
B2 = float(input())
print("input C2")
C2 = float(input())
x = (C1*B2 - B1*C2)/(A1*B2 - B1*A2)
y = (A1*C2 - C1*A2)/(A1*B2 - B1*A2)
print("X = "+str(x)+" and Y = "+str(y))
